{
  "name": "qcrafal/click-meeting",
  "description": "Rest client for ClickMeeting api",
  "keywords": [
    "http",
    "rest",
    "web service",
    "curl",
    "client",
    "HTTP client",
    "ClickMeeting",
    "ClickWebinar"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rafal Kuc",
      "email": "qc.rafal@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ClickMeeting\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ClickMeeting\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "guzzlehttp/guzzle": "~6.2"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Allows more advanced requests interface"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
